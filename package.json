{
  "name": "@kteehan/photo-metadata-extractor",
  "version": "2.0.2",
  "description": "React component for extracting and displaying EXIF photo metadata",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/photo-metadata-extractor.umd.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/photo-metadata-extractor.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/photo-metadata-extractor.es.js",
      "require": "./dist/photo-metadata-extractor.umd.js",
      "default": "./dist/photo-metadata-extractor.es.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet --ignore-pattern dist/",
    "test": "vitest",
    "prepublishOnly": "npm run lint && npm test -- --run && npm run build"
  },
  "keywords": [
    "react",
    "exif",
    "metadata",
    "photo",
    "image"
  ],
  "author": "kristeehan <kristeehan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kteehan/photo-metadata-extractor.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^12.9.1",
    "prettier": "3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "exifreader": "^4.14.1",
    "lucide-react": "^0.539.0"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "bugs": {
    "url": "https://github.com/kteehan/photo-metadata-extractor/issues"
  },
  "homepage": "https://github.com/kteehan/photo-metadata-extractor#readme"
}
